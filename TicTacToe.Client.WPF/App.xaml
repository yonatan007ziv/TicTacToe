<Application x:Class="TicTacToe.Client.WPF.App"			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"			 xmlns:viewModels="clr-namespace:TicTacToe.Client.WPF.MVVM.ViewModels"			 xmlns:views="clr-namespace:TicTacToe.Client.WPF.MVVM.Views"			 Startup="Application_Startup">	<Application.Resources>		<DataTemplate DataType="{x:Type viewModels:MainMenuViewModel}">			<views:MainMenuView />		</DataTemplate>		<DataTemplate DataType="{x:Type viewModels:SingleplayerViewModel}">			<views:SingleplayerView />		</DataTemplate>		<DataTemplate DataType="{x:Type viewModels:MultiplayerViewModel}">			<views:MultiplayerView />		</DataTemplate>		<DataTemplate DataType="{x:Type viewModels:SingleplayerGameEasyViewModel}">			<views:SingleplayerGameView  />		</DataTemplate>				<DataTemplate DataType="{x:Type viewModels:SingleplayerGameMediumViewModel}">			<views:SingleplayerGameView  />		</DataTemplate>				<DataTemplate DataType="{x:Type viewModels:SingleplayerGameImpossibleViewModel}">			<views:SingleplayerGameView />		</DataTemplate>	</Application.Resources></Application>